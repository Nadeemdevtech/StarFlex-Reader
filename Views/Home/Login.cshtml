@{ Layout = null; }
<!doctype html>
<html lang="en">
<head>
    <title>Reporting Panel</title>
    <meta charset="utf-8">
    <link rel="icon" href="~/css/Content/Login/images/navicosoft-logo-png-02-12.png">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

@*<link href="https://fonts.googleapis.com/css?family=Lato:300,400,700,900&display=swap" rel="stylesheet">*@

    @*<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">*@

    <link rel="stylesheet" href="~/css/Content/Login/css/style.css">
<link href="~/css/Content/stackPath.css" rel="stylesheet" />
    <link href="~/css/Content/GoogleApi's.css" rel="stylesheet" />

</head>
<body>
    <section class="ftco-section">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-md-12 col-lg-10">
                    <div class="wrap d-md-flex">
                        <div class="text-wrap p-4 p-lg-5 text-center d-flex align-items-center order-md-last">
                            <div class="text w-100">
                                <h2>Welcome to login</h2>
                            </div>
                        </div>
                        <div class="login-wrap p-4 p-lg-5">
                            <div class="d-flex">
                                <div class="w-100">
                                    <h3 class="mb-4">Sign In</h3>
                                </div>
                            </div>
                            <form action="/Home/Login" method="post" class="signin-form">
                            @*<form action="/sdgpos/Home/Login" method="post" class="signin-form">*@
                                <div class="form-group mb-3">
                                    <label class="label" for="name">Username: User</label>
                                    <input type="text" class="form-control" placeholder="Username" name="uname" required>
                                </div>
                                <div class="form-group mb-3">
                                    <label class="label" for="password">Password: 56789</label>
                                    <input type="password" class="form-control" placeholder="Password" name="pswd" required>
                                </div>
                                <div class="form-group">
                                    <button type="submit" class="form-control btn btn-primary submit px-3">Sign In</button>
                                </div>
                                <div class="form-group d-md-flex">

                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <script src="~/css/Content/Login/js/jquery.min.js"></script>
    <script src="~/css/Content/Loginjs/popper.js"></script>
    <script src="~/css/Content/Loginjs/bootstrap.min.js"></script>
    <script src="~/css/Content/Loginjs/main.js"></script>

</body>
</html>

